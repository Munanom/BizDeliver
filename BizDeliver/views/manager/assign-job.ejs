<h2>Assign A Delivery</h2>
<form action="/assign-job" method="post">
    <div class="form-group">
        <label for="contactName">Parcel Contact Name</label>
        <input type="text" class="form-control" id="contactName" name="contactName" required>
    </div>
    <div class="form-group">
        <label for="location">Location</label>
        <input type="text" class="form-control" id="location" name="location" required>
    </div>
    <div class="form-group">
        <label for="contactInfo">Contact Info</label>
        <input type="text" class="form-control" id="contactInfo" name="contactInfo" required>
    </div>
    <div class="form-group">
        <label for="jobDescription">Job Description</label>
        <input type="text" class="form-control" id="jobDescription" name="jobDescription" required>
    </div>
    <button type="submit" class="btn btn-primary">Assign Job</button>
</form>
<a href="/manager" class="btn btn-secondary mt-3">Back to Dashboard</a>







